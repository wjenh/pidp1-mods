#ifndef DINSTRUCTS_H
#define DINSTRUCTS_H
// The few implemented PDP1-D instructions
// Bit positions
#define liab 20
#define laib 40
#define lswb 60
#define swpb 60
#define cmib 4000
#define snib 4000
#define szib 14000
// Instruction form
#define lia 760000|liab
#define lai 760000|laib
#define lsw 760000|lswb
#define swp 760000|swpb
#define cmi 760000|cmib
#define sni 640000|snib
#define szi 640000|szib
#endif
